<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.EvolutionGeneratorController">
   <children>
      <VBox layoutX="94.0" layoutY="69.0" prefHeight="264.0" prefWidth="167.0">
         <children>
            <Label text="Width:">
               <font>
                  <Font size="17.0" />
               </font>
            </Label>
            <TextField fx:id="width" onKeyTyped="#changeWidth" text="20" />
            <Label prefHeight="26.0" prefWidth="58.0" text="Height:">
               <font>
                  <Font size="17.0" />
               </font>
            </Label>
            <TextField fx:id="height" onKeyTyped="#changeHeight" text="20" />
            <Label prefHeight="26.0" prefWidth="118.0" text="Jungle Width:">
               <font>
                  <Font size="17.0" />
               </font>
            </Label>
            <TextField fx:id="jungleWidth" onKeyTyped="#changeJungleWidth" text="8" />
            <Label prefHeight="26.0" prefWidth="118.0" text="Jungle Height:">
               <font>
                  <Font size="17.0" />
               </font>
            </Label>
            <TextField fx:id="jungleHeight" onKeyTyped="#changeJungleHeight" text="8" />
            <Label prefHeight="26.0" prefWidth="164.0" text="Energy Lost Per Day:">
               <font>
                  <Font size="17.0" />
               </font>
            </Label>
            <TextField fx:id="energyLostPerDay" onKeyTyped="#changeEnergyLostPerDay" text="1" />
         </children>
      </VBox>
      <Label layoutX="142.0" layoutY="14.0" prefHeight="47.0" prefWidth="317.0" text="Evolution Generator">
         <font>
            <Font name="System Bold" size="32.0" />
         </font>
      </Label>
      <VBox layoutX="317.0" layoutY="69.0" prefHeight="290.0" prefWidth="180.0">
         <children>
            <Label text="Energy From Grass:">
               <font>
                  <Font size="17.0" />
               </font>
            </Label>
            <TextField fx:id="energyReceivedFromGrass" onKeyTyped="#changeEnergyFromGrass" text="5" />
            <Label prefHeight="26.0" prefWidth="184.0" text="Grass Per Day:">
               <font>
                  <Font size="17.0" />
               </font>
            </Label>
            <TextField fx:id="quantityOfGrassPerDay" onKeyTyped="#changeGrassPerDay" text="15" />
            <Label prefHeight="26.0" prefWidth="136.0" text="Animals At Start:">
               <font>
                  <Font size="17.0" />
               </font>
            </Label>
            <TextField fx:id="quantityOfAnimalsAtStart" onKeyTyped="#changeAnimalsAtStart" text="15" />
            <Label prefHeight="26.0" prefWidth="126.0" text="Starting Energy:">
               <font>
                  <Font size="17.0" />
               </font>
            </Label>
            <TextField fx:id="startingEnergy" onKeyTyped="#changeStartingEnergy" text="20" />
            <Label prefHeight="26.0" prefWidth="164.0" text="Refresh time(ms):">
               <font>
                  <Font size="17.0" />
               </font>
            </Label>
            <TextField fx:id="refresh" onKeyTyped="#changeRefreshTime" text="200" />
         </children>
      </VBox>
      <Button layoutX="251.0" layoutY="350.0" mnemonicParsing="false" onAction="#Pause" text="Pause/Unpause" />
      <Button layoutX="403.0" layoutY="350.0" mnemonicParsing="false" onAction="#startTwoMaps" text="Start Two Map" />
      <Button layoutX="95.0" layoutY="347.0" mnemonicParsing="false" onAction="#StartOneMapClicked" text="Start One Map" />
   </children>
</AnchorPane>
